{
  "name": "lumina-note",
  "version": "0.5.17",
  "private": true,
  "type": "module",
  "scripts": {
    "sync-version": "node scripts/sync_version.mjs",
    "prebuild": "node scripts/sync_version.mjs",
    "dev": "vite",
    "bundle-node": "node scripts/bundle_node.mjs",
    "generate-latest": "ts-node scripts/generate_latest_json.ts",
    "build": "tsc && vite build",
    "pretauri": "node scripts/sync_version.mjs",
    "preview": "vite preview",
    "release:prepare": "node scripts/release_prepare.mjs",
    "tauri": "tauri",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:e2e:webkit": "WEBKIT_E2E=1 vitest --run src/__tests__/codemirrorSelectAll.webkit.e2e.test.ts",
    "test:repro:flashcard": "vitest --run src/stores/useFlashcardStore.repro.test.ts src/components/flashcard/FlashcardView.repro.test.tsx src/components/flashcard/FlashcardReview.repro.test.tsx src/services/markdown/frontmatter.flashcard.repro.test.ts src/__tests__/flashcardIntentContract.repro.test.ts src/__tests__/flashcardRouterPrompt.repro.test.ts",
    "test:repro:flashcard:e2e": "FLASHCARD_E2E=1 vitest --run src/__tests__/flashcardReview.chromium.e2e.test.ts",
    "test:agent-eval": "npx ts-node --esm tests/agent-eval/runner.ts",
    "test:agent-eval:basic": "npx ts-node --esm tests/agent-eval/runner.ts --category=basic",
    "test:agent-eval:complex": "npx ts-node --esm tests/agent-eval/runner.ts --category=complex"
  },
